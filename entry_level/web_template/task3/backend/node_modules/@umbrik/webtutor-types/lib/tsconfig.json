  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [],
    "strict": true,
    "noLib": true,
    "noEmit": true,
    "baseUrl": "lib",
    "outDir": "../build"
  },
  "include": [
    "**/*.d.ts",
    ".eslintrc.json"
  ]
